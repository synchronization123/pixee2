import base64
import zlib
import marshal
import textwrap

ROUNDS = 10

INPUT_FILE = "your_script.pyw"
OUTPUT_FILE = "your_script_encrypted.pyw"

with open(INPUT_FILE, "r", encoding="utf-8") as f:
    source = f.read()

data = source

for _ in range(ROUNDS):
    code_obj = compile(data, "<protected>", "exec")
    marshaled = marshal.dumps(code_obj)
    compressed = zlib.compress(marshaled)
    data = base64.b64encode(compressed).decode("ascii")

payload = data  # SAFE ASCII STRING

final_code = f"""
import base64, zlib, marshal

data = "{payload}"

decoded = base64.b64decode(data)
decompressed = zlib.decompress(decoded)
code = marshal.loads(decompressed)

exec(code)
"""

final_code = textwrap.dedent(final_code)

with open(OUTPUT_FILE, "w", encoding="utf-8") as f:
    f.write(final_code)

print("✔ Encryption successful")
print(f"✔ Output file: {OUTPUT_FILE}")